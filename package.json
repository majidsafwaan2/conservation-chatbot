{
  "name": "conservation-chatbot",
  "version": "1.0.3",
  "description": "Interactive AI chatbot for endangered animals - Perfect for NGOs and conservation organizations",
  "main": "dist/conservation-chatbot.umd.js",
  "module": "dist/conservation-chatbot.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/conservation-chatbot.es.js",
      "require": "./dist/conservation-chatbot.umd.js"
    },
    "./animals": "./src/animals/index.js",
    "./styles": "./src/styles/index.js"
  },
  "files": [
    "dist",
    "src",
    "examples"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "npm run build && npm test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "node test/basic.test.js",
    "docs": "echo \"Documentation is in README.md\""
  },
  "keywords": [
    "chatbot",
    "conservation",
    "endangered-animals",
    "ngo",
    "ai",
    "gemini",
    "javascript",
    "npm-package"
  ],
  "author": "Safwaan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/majidsafwaan2/conservation-chatbot.git"
  },
  "homepage": "https://github.com/majidsafwaan2/conservation-chatbot#readme",
  "bugs": {
    "url": "https://github.com/majidsafwaan2/conservation-chatbot/issues"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "vite": "^5.4.19"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
