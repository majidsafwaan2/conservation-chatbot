{
  "name": "conservation-chatbot",
  "version": "1.0.2",
  "description": "A plug-and-play AI chatbot for endangered animals - Easy to integrate, customizable, and ready for conservation organizations",
  "main": "dist/conservation-chatbot.umd.js",
  "module": "dist/conservation-chatbot.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/conservation-chatbot.es.js",
      "require": "./dist/conservation-chatbot.umd.js"
    },
    "./animals": "./src/animals/index.js",
    "./styles": "./src/styles/index.js"
  },
  "files": [
    "dist",
    "src/animals",
    "src/styles",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "node test/basic.test.js",
    "docs": "echo \"Documentation is in README.md\""
  },
  "keywords": [
    "chatbot",
    "AI",
    "conservation",
    "endangered-animals",
    "wildlife",
    "environmental-education",
    "react-component",
    "vanilla-js",
    "gemini-ai",
    "chat-widget",
    "conservation-education",
    "wildlife-conservation",
    "environmental-chatbot",
    "animal-chatbot",
    "educational-chatbot"
  ],
  "author": "Conservation Chatbot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/conservation-chatbot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/conservation-chatbot/issues"
  },
  "homepage": "https://github.com/your-username/conservation-chatbot#readme",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "vite": "^5.3.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
